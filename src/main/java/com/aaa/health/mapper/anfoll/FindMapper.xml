<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aaa.health.mapper.anfoll.FindMapper">
    <delete id="delete" parameterType="String">
        delete from antenatalFollowup where anfoId in(${value})
    </delete>


    <!-- 分页获取用户列表 -->
    <select id="findList" parameterType="Map" resultType="com.aaa.health.entity.anfoll.AnteFollow">
        select anfoId,anfoGestational,anfoParity,anfoName,fillformTime,anfoState from antenatalFollowup
        where 1 = 1
        <if test="offset != null and pageSize != null">
            limit #{offset},#{pageSize}
        </if>
    </select>
    <select id="getTotal" parameterType="Map" resultType="Integer">
        select count(*) from antenatalFollowup where 1 = 1
    </select>

</mapper>